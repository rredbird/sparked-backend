<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <File name="fileAppender" fileName="logs/log.log">
        <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level - %msg%n"/>
    </File>
</Appenders>




  <Loggers>
    <Root level="warn">
    </Root>

    <Logger name="coda.shared" level="debug">
        <AppenderRef ref="fileAppender" />
    </Logger>
  </Loggers>
</Configuration>